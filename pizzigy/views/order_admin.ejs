    <div class="container-fluid" style="padding: 0% 10%">
      <div class="row px-5">
        <div class="col-md-7">
          <% for(var i=0; i < pending_orders.length; i++) { %>
          <div class="">
            <div class="row">
              <div class="col-md-6">
                <h3 class="pt-1"><%=pending_orders[i].itemId.name%></h3>

                <form
                  action="/assign/<%= pending_orders[i].itemId %>/<%= admin._id %>"
                  method="post"
                >
                  <button
                    type="submit"
                    class="btn btn-danger mx-2"
                    name="remove"
                  >
                    Assign
                  </button>
                </form>
              </div>
            </div>
          </div>
          <br /><br />
          <% } %>
      </div>
    </div>
